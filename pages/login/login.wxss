.main {
  width: 100%;
  height: 100vh;
  background-color: #f2f2f2;
  padding-top: 50px;
}

.main .top {
  width: 100%;
  display: flex;
  align-items: center;
  font-size: 28px;
  display: flex;
  justify-content: center;
  align-items: center;
  margin-bottom: 50px;
}

.main .top .icon-yonghu {
  font-size: 33px;
  border: 1px solid #005826;
  color: #005826;
  padding: 1px;
  border-radius: 50%;
  display: flex;
  margin-right: 10px;
}

.main .form {
  display: flex;
  flex-direction: column;
  align-items: center;
  margin-bottom: 10px;
}

.main .form .form_content {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  width: 617.308rpx;
  height: 292px;
  background-color: white;
  border-radius: 19.231rpx;
}

.main .form .form_content .form_item {
  display: flex;
  position: relative;
  height: 105.769rpx;
  margin-bottom: 38.462rpx;
  position: relative;
}

.main .form .form_content .form_item .forgetPassword {
  text-decoration: underline;
  font-size: 26.923rpx;
  color: #005826;
  position: absolute;
  right: 0;
  top: 60px;
}

.main .form .form_content .form_item input {
  width: 463.462rpx;
  border-bottom: 3.846rpx solid #005826;
  font-size: 34.615rpx;
  height: 96.154rpx;
}

.main .form .form_content .form_item .iconfont {
  font-size: 46.154rpx;
  position: absolute;
  right: 0;
  top: 23.077rpx;
}

.main .form .form_content .bottom_info {
  display: flex;
  font-size: 14px;
  align-items: center;
  margin-top: 25px;
}

.main .form .form_content .bottom_info .icon-duigou {
  margin-right: 10px;
  color: #005826;
}

.main .form .form_content_done {
  width: 321px;
  height: 377px;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  padding: 30px 0;
  box-sizing: border-box;
  align-items: center;
  background-color: white;
  border-radius: 10px;
}

.main .form .form_content_done .form_content_done_d1 {
  display: flex;
  flex-direction: column;
  justify-content: space-evenly;
  height: 70%;
  font-size: 18px;
  align-items: center;
}

.main .form .form_content_done .form_content_done_d1 .icon-duigou {
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 80px;
  color: #BD3124;
  margin-top: 30px;
}

.main .form .form_content_done .form_content_done_d1 .form_content_done_d1_grade {
  font-size: 20px;
  color: #005826;
}

.main .form .form_content_done .form_content_done_d2 {
  font-size: 14px;
  text-decoration: underline;
  color: #005826;
}

.main .bottom {
  width: 100%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}

.main .bottom .loginAndRegister .bottom_item {
  width: 241px;
  height: 48px;
  display: flex;
  align-items: center;
  justify-content: center;
  background-color: #005826;
  border-radius: 30px;
  color: white;
  font-size: 16px;
  margin-top: 10px;
}

.main .bottom .loginAndRegister .toSregister {
  background-color: #9A9A9A;
}

.main .bottom .wechatLogo {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  font-size: 14px;
  color: #005826;
}

.main .bottom .wechatLogo .icon-weixin {
  width: 45px;
  height: 45px;
  font-size: 40px;
  border: 1px solid #005826;
  margin: 10px 0;
  display: flex;
  align-items: center;
  justify-content: center;
  border-radius: 50%;
  padding: 3px;
  color: #005826;
}
