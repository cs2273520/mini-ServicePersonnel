.main {
  width: 100%;
  min-height: 100vh;
  background-color: #ccc;
}

.main .content {
  width: 100%;
  padding: 10px;
  background-color: rgba(0, 77, 33, 0.11);
  margin: 10px 0;
  border-radius: 10px;
  box-sizing: border-box;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.main .content .content_title {
  width: 100%;
  display: flex;
  font-size: 16px;
}

.main .content .content_center {
  margin: 10px 0;
  max-height: 300px;
  overflow: scroll;
  position: relative;
}

.main .content .content_center .content_center_item {
  margin-bottom: 15px;
  width: 355px;
  height: 85px;
  background-color: white;
  border-radius: 5px;
  display: flex;
  justify-content: space-evenly;
  padding: 10px 0;
  box-sizing: border-box;
}

.main .content .content_center .content_center_item .content_center_item_left {
  display: flex;
  align-items: center;
  font-size: 14px;
}

.main .content .content_center .content_center_item .content_center_item_center {
  display: flex;
  align-items: center;
}

.main .content .content_center .content_center_item .content_center_item_center .point {
  width: 20px;
  height: 20px;
  background-color: #005826;
  border-radius: 50%;
  display: flex;
  align-items: center;
}

.main .content .content_center .content_center_item .content_center_item_right {
  font-size: 14px;
  display: flex;
  flex-direction: column;
  justify-content: space-evenly;
}

.main .content .content_center .content_center_item .content_center_item_right view:nth-of-type(1) {
  color: #9A9A9A;
}

.main .content .content_center_line {
  position: absolute;
  width: 100%;
  height: 2px;
  background-color: #0F40F5;
  top: 43%;
  width: 85.5%;
}

.main .content .content_bottom {
  width: 100%;
  display: flex;
  align-items: flex-end;
  flex-direction: column;
}

.main .content .content_bottom .content_bottom_item {
  display: flex;
  align-items: center;
  font-size: 14px;
  color: #BD3124;
}

.main .content .content_bottom .content_bottom_item view:nth-child(1) {
  width: 15px;
  height: 15px;
  background-color: #BD3124;
  margin-right: 10px;
  border-radius: 50%;
}

.main .content .content_bottom .content_bottom_item:nth-of-type(2) {
  color: #005826;
}

.main .content .content_bottom .content_bottom_item:nth-of-type(2) view:nth-of-type(1) {
  width: 15px;
  height: 15px;
  background-color: #005826;
  margin-right: 10px;
  border-radius: 50%;
}
